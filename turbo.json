{
  "$schema": "https://turborepo.org/schema.json",
  "baseBranch": "origin/main",
  "globalDependencies": [
    "tsconfig.json",
    "package.json",
    ".eslintrc.json",
    ".eslintignore"
  ],
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "@sharingan/core#build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**"
      ],
      "inputs": [
        "src/**/*.tsx",
        "src/**/*.ts",
        "*.ts",
        "codegen.yml",
        ".eslintignore",
        "*.json"
      ],
      "env": [
        "NODE_ENV",
        "DATABASE_URL"
      ]
    },
    "@sharingan/web#build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        ".next/**",
        "dist/**"
      ],
      "inputs": [
        "src/**/*.tsx",
        "src/**/*.ts",
        "__tests__/**/*.tsx",
        "__tests__/**/*.ts",
        "__mocks__/**/*.js",
        "*.ts",
        "next.config.js",
        "codegen.yml",
        ".eslintignore",
        "*.json"
      ],
      "env": [
        "NODE_ENV",
        "NEXT_PUBLIC_SERVER_URL"
      ]
    },
    "test": {
      "dependsOn": [
        "^build"
      ],
      "outputs": []
    },
    "build:prod": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "build/**"
      ]
    },
    "lint": {
      "outputs": []
    },
    "clean": {
      "outputs": []
    },
    "dev": {
      "cache": false
    }
  }
}
